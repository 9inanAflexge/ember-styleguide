bracketSpacing: true
endOfLine: lf
jsxBracketSameLine: false
printWidth: 100
semi: true
singleQuote: true
tabWidth: 2
trailingComma: es5
useTabs: false
overrides:
  - files: '*.yaml'
    options:
      parser: yaml
      tabWidth: 2
  - files: '.prettierrc.yaml'
    options:
      parser: yaml
      tabWidth: 2
  - files: '.ember-cli'
    options:
      parser: json
      tabWidth: 4
  - files: '.watchmanconfig'
    options:
      parser: json
